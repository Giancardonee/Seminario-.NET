    
    swap(10,20); 
    
    static void swap (int n1, int n2){
        int aux = n1;
        Console.WriteLine("Valores antes del swap: "); 
        Console.WriteLine("Numero1: "+n1); 
        Console.WriteLine("Numero2: "+n2); 
        n1 = n2; 
        n2 = aux;
        Console.WriteLine("Valores despues del swap: "); 
        Console.WriteLine("Numero1: "+n1); 
        Console.WriteLine("Numero2: "+n2); 
    }